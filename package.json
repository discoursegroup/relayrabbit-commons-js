{
  "name": "@discoursegroup/relayrabbit-commons-js",
  "version": "0.0.5",
  "description": "Common utilities and interfaces used by the RelayRabbit JS projects",
  "main": "build/index.js",
  "typings": "build/types/index.d.ts",
  "module": "build/index.js",
  "scripts": {
    "start": "webpack serve --open --config webpack.dev.js",
    "build": "NODE_ENV=production webpack --config webpack.prod.js && tsc",
    "build:dev": "webpack --config webpack.dev.js",
    "test": "run-s build test:*",
    "test:lint": "eslint src --ext .ts",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:spelling": "cspell \"{README.md,.github/*.md,src/**/*.ts}\"",
    "test:unit": "jest --runInBand --passWithNoTests",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version": "standard-version",
    "reset-hard": "git clean -dfx && git reset --hard && npm i",
    "prepare-release": "run-s reset-hard test doc:html version doc:publish",
    "release": "standard-version",
    "mockpublish": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "prepare": "husky install",
    "upgrade-dependencies": "ncu -u && npm install"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/discoursegroup/relayrabbit-commons-js.git"
  },
  "keywords": [
    "crypto",
    "binary",
    "json",
    "common utils",
    "crypto",
    "io",
    "Discourse Group"
  ],
  "author": "Discourse Group LLC",
  "maintainers": [
    "Swapnil Patel <delta2wye@gmail.com> (https://github.com/delta2wye)",
    "Raship Shah <shahraship@gmail.com> (https://github.com/shahraship)"
  ],
  "license": "SEE LICENSE IN README",
  "bugs": {
    "url": "https://github.com/discoursegroup/relayrabbit-commons-js/issues"
  },
  "homepage": "https://github.com/discoursegroup/relayrabbit-commons-js#readme",
  "devDependencies": {
    "@discoursegroup/commons-test-js": "^0.0.4",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.1.0",
    "cspell": "^4.1.0",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-webpack-plugin": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "mini-css-extract-plugin": "^2.4.5",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.2.0",
    "prettier": "^2.1.1",
    "service-worker-webpack": "^0.0.11",
    "standard-version": "^9.0.0",
    "terser-webpack-plugin": "^5.2.5",
    "ts-jest": "^27.1.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^9.0.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "workbox-routing": "^6.4.2",
    "workbox-strategies": "^6.4.2",
    "workbox-webpack-plugin": "^6.4.2"
  },
  "browser": {
    "crypto": false
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "files": [
    "README.md",
    "package.json",
    "build/index.js",
    "build/types/index.d.ts",
    "build/types/models/rr-payload.d.ts",
    "build/types/transformers/transformer.d.ts"
  ],
  "directories": {
    "test": "test",
    "src": "src"
  },
  "peerDependencies": {
    "@discoursegroup/commons-js": "^0.0.10"
  }
}
